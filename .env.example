# Application URLs
# Not needed in development
DOMAIN=example.com
APP_URL=custom@domain.com
AUTH_URL=custom@domain.com

# Main Database Configuration (Supabase)
# Get these values from your Supabase project settings
DATABASE_URL=postgresql://postgres.username:password@your-supabase-host.pooler.supabase.com:6543/postgres
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Authentication Database Configuration (Separate Supabase instance)
# Get these values from your authentication Supabase project settings
AUTH_DATABASE_URL=postgresql://postgres.username:password@your-auth-supabase-host.pooler.supabase.com:6543/postgres
NEXT_PUBLIC_AUTH_SUPABASE_URL=https://your-auth-project-id.supabase.co
NEXT_PUBLIC_AUTH_SUPABASE_ANON_KEY=your_auth_supabase_anon_key_here

# PayPal Configuration
# Get these from your PayPal Developer Dashboard
# Use sandbox credentials for development, live credentials for production
PAYPAL_CLIENT_ID="your_paypal_client_id_here"
PAYPAL_CLIENT_SECRET="your_paypal_client_secret_here"
PAYPAL_API_BASE="https://api-m.sandbox.paypal.com"  # Use https://api-m.paypal.com for production
PAYPAL_WEBHOOK_ID=your_webhook_id_from_paypal_dashboard

# Resend Configuration
# Get these from your Resend Api Keys Page
RESEND_API_KEY="your_resend_api_key_here"

# Crisp Configuration
NEXT_PUBLIC_CRISP_WEBSITE_ID="your_crisp_api_key_here"

# Google
GEMINI_API_KEY="your_gemini_api_key_here"
